 {
  "manifest_version": 2,
  "name": "ITC-Text Extractor",
  "version": "0.1",
  "permissions": ["activeTab"],
  "description": "Scraps Your Product Text and Image",
  "icons": {"16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png" },
  "browser_action": {
   "default_icon": "tab-icon.png",
   "default_title": "Highlight the important stuff",
   "default_popup": "popup.html"
  }
}

